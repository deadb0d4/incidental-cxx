// autogenerated every build
#include <debug/debug.hpp>

{% if debug_log_filename %}
  #include <fstream>
{% endif %}

namespace debug {

std::ostream& Ostream() {
  {% if debug_log_filename %}
    static std::ofstream fout("{{ debug_log_filename }}");
    return fout;
  {% else %}
    return std::cerr;
  {% endif %}
}

}  // namespace debug
